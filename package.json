{
  "name": "@fivexlabs/fivui",
  "version": "0.9.3",
  "description": "A modern, lightweight UI component library CLI for React and Next.js applications, built with TailwindCSS",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "fivui": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "registry",
    "templates",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:cli",
    "build:cli": "tsup --config tsup.cli.config.ts",
    "dev": "tsup --config tsup.cli.config.ts --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "nextjs",
    "tailwindcss",
    "typescript",
    "design-system",
    "shadcn",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fivexlabs/fivui.git"
  },
  "bugs": {
    "url": "https://github.com/fivexlabs/fivui/issues"
  },
  "homepage": "https://github.com/fivexlabs/fivui#readme",
  "author": "Fivex Labs",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "tailwindcss": ">=4.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/prompts": "^2.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
